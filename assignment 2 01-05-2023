ind={}
n=int(input("enter no of student"))
for i in range(1,n+1):
    l=[]
    sno=int(input("enter student number"))
    sname=input("enter student name")
    group=input("enter your group")
    college=input("enter you college name")
    sphonenumber=input("enter your student phone number")
    l.append(sname)
    l.append(group)
    l.append(college)
    l.append(sphonenumber)
    ind[sno]=l
    print(ind)
